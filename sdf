

# 좌표 n, a
# 지워질 좌표 n~(0,19, a제외), (0,19,n제외)~a


d = [[0] * 20 for _ in range(20)]

n = int(input())
for i in range(n) :
    a = tuple(map(int, input().split()))

    x = a[0]  
    y = a[1]  
    
    if d[x-1][:y-1:]==0 :
        d[x-1][:y-1:] = 1
    else: 
        d[x-1][:y-1:] = 0
 
    if d[:x-1:][y-1]==0 : 
        d[:x-1:][y-1] = 1   
    else: 
        d[:x-1:][y-1]=0
    
for i in range(1,20) :
    
    for j in range(1,20) :
        print(d[i][j] , end = ' ')
    print()
